                <xs:element ref="FileName" minOccurs="0"/>
                <xs:element ref="FilePath" minOccurs="0"/>
                <xs:element ref="CreatedDate"/>
            </xs:sequence>
            <xs:attribute name="active" type="AT_1"/>
        </xs:complexType>
    </xs:element>
    <!-- contactinfo -->
    <xs:element name="ContactInfo">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Person" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- livepublish -->
    <xs:element name="LivePublish">
        <xs:complexType>
