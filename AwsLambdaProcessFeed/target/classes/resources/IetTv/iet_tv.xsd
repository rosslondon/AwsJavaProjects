                <xs:element ref="Password"/>
                <xs:element ref="BitrateList"/>
            </xs:sequence>
            <xs:attribute name="active" type="AT_1" use="required"/>
        </xs:complexType>
    </xs:element>
    <!-- Bitratelist -->
    <xs:element name="BitrateList">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Bitrate" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
