<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

    <!--video-->
    <xs:element name="Video">
        <xs:complexType>
            <xs:all>
                <xs:element ref="HomePageVideo" minOccurs="0"/>
                <xs:element ref="VideoNumber" minOccurs="0"/>  
                <xs:element ref="VideoStatus"/>
                <xs:element ref="VideoURL"/>                
                <xs:element ref="VideoType" minOccurs="0"/>
				<xs:element ref="VideoSource" minOccurs="0"/>
                <xs:element ref="CreationInfo"/>
                <xs:element ref="ModifiedInfo" minOccurs="0"/>
                <xs:element ref="BasicInfo"/>
                <xs:element ref="AdvanceInfo" minOccurs="0"/>
                <xs:element ref="KeyWordInfo" minOccurs="0"/>
                <xs:element ref="Attachments" minOccurs="0"/>
                <xs:element ref="Speakers" minOccurs="0"/>
                <xs:element ref="ClientInfo" minOccurs="0"/>
                <xs:element ref="ProductionInfo" minOccurs="0"/>
                <xs:element ref="UploadVideo" minOccurs="0"/>
                <xs:element ref="PublishInfo" minOccurs="0"/>
                <xs:element ref="Events" minOccurs="0"/>
                <xs:element ref="SeriesList" minOccurs="0"/>
                <xs:element ref="VideoInspec" minOccurs="0"/>
                <xs:element ref="IETDigitalLibrary" minOccurs="0"/>
            </xs:all>
            <xs:attribute name="ID" type="xs:string"/>
        </xs:complexType>
    </xs:element>

    <!-- creation info -->
    <xs:element name="CreationInfo">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Date"/>
                <xs:element ref="Person" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- modifiedInfo -->
    <xs:element name="ModifiedInfo">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Date"/>
                <xs:element ref="Person" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- basicinfo -->
    <xs:element name="BasicInfo">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Title"/>
                <xs:element ref="VideoCategory" minOccurs="0"/>
                <xs:element ref="Abstract" minOccurs="0"/>
                <xs:element ref="ShortDescription" minOccurs="0"/>
                <xs:element ref="VideoCreatedDate" minOccurs="0"/>
                <xs:element ref="ChannelMapping"/>
                <xs:element ref="PricingDetails" minOccurs="0"/>
                <xs:element ref="PromoDetails" minOccurs="0"/>
                <xs:element ref="CopyrightDetails" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- Advanceinfo -->
    <xs:element name="AdvanceInfo">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="LocationDetails" minOccurs="0"/>
                <xs:element ref="PermissionDetails" minOccurs="0"/>
                <xs:element ref="Subtitles" minOccurs="0"/>
                <xs:element ref="Transcripts" minOccurs="0"/>
                <xs:element ref="Syndication" minOccurs="0"/>
                <xs:element ref="Advert" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- keywordinfo -->
    <xs:element name="KeyWordInfo">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="ChannelKeywordList"/>
                <xs:element ref="CustomKeywordList" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- attachments -->
    <xs:element name="Attachments">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Attachment" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="SponserDetails" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- speakers -->
    <xs:element name="Speakers">
        <xs:complexType>
            <xs:sequence minOccurs="0">
                <xs:element ref="Person" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- clientinfo -->
    <xs:element name="ClientInfo">
        <xs:complexType>
            <xs:sequence minOccurs="0">
                <xs:element ref="Clients" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- Clients (added) -->
    <xs:element name="Clients">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Client" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- productioninfo -->
    <xs:element name="ProductionInfo">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Person" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- UploadVideo-->
    <xs:element name="UploadVideo">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="File" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- Publishinfo -->
    <xs:element name="PublishInfo">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="VideoPublish" minOccurs="0"/>
                <xs:element ref="LivePublish" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Events -->
    <xs:element name="Events">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Event" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Series -->
    <xs:element name="SeriesList">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Series" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- VideoInspec -->
    <xs:element name="VideoInspec">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Kwd-Group" minOccurs="1" maxOccurs="unbounded"/>
                <xs:element ref="Inspec"/>
            </xs:sequence>
            <xs:attribute name="ID" type="xs:string"/>
        </xs:complexType>
    </xs:element>

    <!-- kwd-group -->
    <xs:element name="Kwd-Group">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Kwd" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="Compound-Kwd" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attribute name="kwd-group-type" type="xs:string" use="required"/>
        </xs:complexType>
    </xs:element>
    <!-- compound kwd -->
    <xs:element name="Compound-Kwd">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Compound-Kwd-Part" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- compound-kwd-part -->
    <xs:element name="Compound-Kwd-Part">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="content-type" type="xs:string"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <!--Inspec-->
    <xs:element name="Inspec">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="AccessionNumber" minOccurs="0"/>
                <xs:element ref="AccessionNumberForPublishedArticle" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- IETDigitallibrary -->
    <xs:element name="IETDigitalLibrary">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="EventNo"/>
                <xs:element ref="DOI" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!--Person-->
    <xs:element name="Person">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Title" minOccurs="0"/>
                <xs:element ref="Name"/>
                <xs:element ref="PostNominal" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="PhoneNo" minOccurs="0"/>
                <xs:element ref="MobileNo" minOccurs="0"/>
                <xs:element ref="Affiliations" minOccurs="0"/>
                <xs:element ref="Organization" minOccurs="0"/>
                <xs:element ref="JobTitle" minOccurs="0"/>
                <xs:element ref="EmailID" minOccurs="0"/>
				<xs:element ref="Website" minOccurs="0"/>
                <xs:element ref="FileName" minOccurs="0"/>
                <xs:element ref="FilePath" minOccurs="0"/>
                <xs:element ref="Biography" minOccurs="0"/>
                <xs:element ref="Description" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="ID" type="xs:string" use="required"/>
            <xs:attribute name="type" type="xs:string"/>
        </xs:complexType>
    </xs:element>
    <!-- Organization -->
    <xs:element name="Organization">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="OrganizationName"/>
                <xs:element ref="Department" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="Description" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="Address" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="Website" minOccurs="0"></xs:element>
            </xs:sequence>
            <xs:attribute name="ID" type="xs:string"/>
        </xs:complexType>
    </xs:element>
    <!-- department -->
    <xs:element name="Department">
        <xs:complexType>
            <xs:sequence minOccurs="0">
                <xs:element ref="DepartmentName"/>
                <xs:element ref="ResponsiblePerson" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="ID" type="xs:string"/>
        </xs:complexType>
    </xs:element>
    <!-- affiliation -->
    <xs:element name="Affiliation">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Organization"/>
            </xs:sequence>
            <xs:attribute name="ID" type="xs:string"/>
        </xs:complexType>
    </xs:element>
    <!-- address -->
    <xs:element name="Address">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Addr-Line" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="Postcode" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="City" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="State" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="Country" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="Phone" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="Fax" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="Email" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="URL" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!--Name-->
    <xs:element name="Name">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Given-Name" maxOccurs="unbounded" minOccurs="0"/>
                <xs:element ref="Surname" minOccurs="0"/>
                <xs:element ref="Suffix" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- location -->
    <xs:element name="Location">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="LocationName"/>
                <xs:element ref="LocationDate" minOccurs="0"/>
                <xs:element ref="LocationDescription" minOccurs="0"/>
                <xs:element ref="Address" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="ID" type="xs:string" use="required"/>
        </xs:complexType>
    </xs:element>
    <!-- file -->
    <xs:element name="File">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="MediaType"/>
                <xs:element ref="Duration" minOccurs="0"/>
                <xs:element ref="UploadStatus" minOccurs="0"/>
                <xs:element ref="FileName" minOccurs="0"/>
                <xs:element ref="FilePath" minOccurs="0"/>
                <xs:element ref="UploadDate" minOccurs="0"/>
                <xs:element ref="Size" minOccurs="0"/>
                <xs:element ref="Format" minOccurs="0"/>
                <xs:element ref="ThumbnailFilepath" minOccurs="0"/>
                <xs:element ref="NotificationCount" minOccurs="0"/>
              
              <xs:element ref="URL" minOccurs="0"/>
                <xs:element ref="Person" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="ftpUpload" type="AT_1" use="required"/>
            <xs:attribute name="streamID" type="xs:string" use="required"/>
            <xs:attribute name="active" type="AT_1" use="required"/>
        </xs:complexType>
    </xs:element>
    <!--event-->
    <xs:element name="Event">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="EventName"/>
                <xs:element ref="Location"/>
                <xs:element ref="Venue"/>
                <xs:element ref="Room" minOccurs="0"/>
                <xs:element ref="StartDate"/>
                <xs:element ref="StartTime" minOccurs="0"/>
                <xs:element ref="EndDate"/>
                <xs:element ref="EndTime" minOccurs="0"/>
                <xs:element ref="Session" minOccurs="0"/>
                <xs:element ref="DisplayQA" minOccurs="0"/>
                <xs:element ref="DisplayPolling" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="ID" type="xs:string" use="required"/>
        </xs:complexType>
    </xs:element>
    <!-- added the below elements -->
    <xs:element name="DisplayQA" type="xs:string"/>
    <xs:element name="DisplayPolling" type="xs:string"/>
    <!--HomePageVideo  -->
    <xs:element name="HomePageVideo">
        <xs:complexType>
            <xs:attribute name="status" type="AT_1" use="optional"/>
        </xs:complexType>
    </xs:element>
    <!-- VideoStatus -->
    <xs:element name="VideoStatus">
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:enumeration value="Draft"/>
                <xs:enumeration value="Withdrawn"/>
                <xs:enumeration value="Published"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <!-- videotype -->
    <xs:element name="VideoType">
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:enumeration value="NonPromo"/>
                <xs:enumeration value="Promo"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <!--venue-->
    <xs:element name="Venue">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="ID" type="xs:string" use="required"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <!-- room -->
    <xs:element name="Room">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="ID" type="xs:string" use="required"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <!-- PromoDetails -->
    <xs:element name="PromoDetails">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Countdown" minOccurs="0"/>
                <xs:element ref="PromoVideo" minOccurs="0"/>
                <xs:element ref="FileName" minOccurs="0"/>
                <xs:element ref="FilePath" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
        </xs:complexType>
    </xs:element>
    <!--client-->
    <xs:element name="Client">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Department" minOccurs="0"/>
                <xs:element ref="Organization" minOccurs="0"/>
                <xs:element ref="ContactInfo" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="type" type="AT_104" use="required"/>
        </xs:complexType>
    </xs:element>
    <!--Period-->
    <xs:element name="Period">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="ID" type="xs:string" use="required"/>
                    <xs:attribute name="type" type="xs:string" use="required"/>
                    <xs:attribute name="value" type="xs:string" use="required"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <!-- Player -->
    <xs:element name="Player">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="ID" type="xs:string" use="required"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <!-- series -->
    <xs:element name="Series">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="SeriesName"/>
                <xs:element ref="Description"/>
				        <xs:element ref="IconImage" minOccurs="0"/>
                <xs:element ref="SeriesCreatedDate" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="ID" type="xs:string" use="required"/>
        </xs:complexType>
    </xs:element>
    <!-- source -->
    <xs:element name="Source">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="ID" type="xs:string" use="required"/>
                    <xs:attribute name="active" type="AT_1"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <!-- adverts -->
    <xs:element name="Advert">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="AdvertName"/>
                <xs:element ref="PrerollFileName" minOccurs="0"/>
                <xs:element ref="PostrollFileName" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="ID" type="xs:string" use="required"/>
            <xs:attribute name="use" type="AT_1" use="required"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="ChannelKeywordList">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Channel" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="CustomKeywordList">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="CustomKeyword" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- channel -->
    <xs:element name="Channel">
        <xs:complexType>
            <xs:all>
                <xs:element ref="Category" minOccurs="0"/>
                <xs:element ref="ChannelName"/>
                <xs:element ref="DOI" minOccurs="0"/>
                <xs:element ref="URL" minOccurs="0"/>
                <xs:element ref="KeywordList" minOccurs="0"/>
            </xs:all>
            <xs:attribute name="ID" type="xs:string" use="required"/>
            <xs:attribute name="default" type="xs:boolean"/>
        </xs:complexType>
    </xs:element>
    <!-- default keyword -->
    <xs:element name="DefaultKeyword">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="ID" type="xs:string" use="required"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <!-- keyword -->
    <xs:element name="CustomKeyword">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="ID" type="xs:string" use="required"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <!-- pricing -->
    <xs:element name="PricingDetails">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="UploadPrice"/>
                <xs:element ref="Price" minOccurs="0"/>
                <xs:element ref="DiscountList" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required"/>
            <xs:attribute name="ID" type="xs:string"/>
        </xs:complexType>
    </xs:element>
    <!-- session -->
    <xs:element name="Session">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="ID" type="xs:string" use="required"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <!-- category -->
    <xs:element name="Category">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="ID" type="xs:string"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <!-- Discountlist -->
    <xs:element name="DiscountList">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Discount" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- discount -->
    <xs:element name="Discount">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="level" type="AT_32" use="required"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <!--<FileName fileType="custom" mediaType="Image">C:\DefaultImage.png</FileName>-->
    <xs:element name="FileName">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="type">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="custom"/>
                                <xs:enumeration value="default"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                    <xs:attribute name="mediaType" type="xs:string"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <!-- keywords -->
    <xs:element name="KeywordList">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="DefaultKeyword" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- subtitle -->
    <xs:element name="Subtitle">
        <xs:complexType>
            <xs:sequence minOccurs="0">
                <xs:element ref="Language"/>
                <xs:element ref="Text"/>
                <xs:element ref="DownloadLink"/>
                <xs:element ref="FileName" minOccurs="0"/>
                <xs:element ref="FilePath"/>
                <xs:element ref="CreatedDate"/>
            </xs:sequence>
            <xs:attribute name="active" type="AT_1"/>
        </xs:complexType>
    </xs:element>
    <!-- countdown -->
    <xs:element name="Countdown">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Date"/>
                <xs:element ref="Time"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- mediatype -->
    <xs:element name="MediaType">
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:enumeration value="Live"/>
                <xs:enumeration value="Video"/>
                <xs:enumeration value="VIDEO"/>
                <xs:enumeration value="video"/>
                <xs:enumeration value="LIVE"/>
                <xs:enumeration value="live"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <!-- subtitles -->
    <xs:element name="Subtitles">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Subtitle" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- attachment -->
    <xs:element name="Attachment">
        <xs:complexType>
            <xs:sequence minOccurs="0">
                <xs:element ref="AttachmentName"/>
                <xs:element ref="FileName"/>
                <xs:element ref="UploadDate"/>
                <xs:element ref="UploadTime"/>
            </xs:sequence>
            <xs:attribute name="ID" type="xs:string"/>
            <xs:attribute name="active" type="AT_1"/>
        </xs:complexType>
    </xs:element>
  <!-- SponserDetails -->
  <xs:element name="SponserDetails">
    <xs:complexType>
      <xs:sequence minOccurs="0">
        <xs:element ref="URL"/>
        <xs:element ref="FileName"/>
      </xs:sequence>     
    </xs:complexType>
  </xs:element>
    <!-- Permission -->
    <xs:element name="PermissionDetails">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Permission" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="VideoThumbnailImage" minOccurs="0"/>
                <xs:element ref="URL" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- permission -->
    <xs:element name="Permission">
        <xs:complexType>
            <xs:sequence minOccurs="0">
                <xs:element ref="Player" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string"/>
            <xs:attribute name="status" type="AT_1"/>
            <xs:attribute name="embed" type="AT_1"/>
        </xs:complexType>
    </xs:element>
    <!-- promovideo -->
    <xs:element name="PromoVideo">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="PromoTitle"/>
